package testing;
import java.util.*;

public class tt {
	public static void main(String args[])
	{
		Scanner in=new Scanner(System.in);
		System.out.println("Enrer an integer");
		int n=in.nextInt();
		
		int a[]=new int[n];
		int l=1;
		int j=0;
		
		while(j<a.length)
		{
			int pc=0;
			for(int i=1;i<l;i++)
			{
				if(l%i==0)
					pc++;
			}
			
			if(pc==2)
			{
				a[j]=l;
				j++;
			}
			l++;
		}
		
		for(int i=0;i<a.length;i++)
		{
			System.out.println(a[i]);
		}
	
		}
		
}