package testing;
import java.util.*;

public class tt {
	public static void main(String args[])
	{
		Scanner in=new Scanner(System.in);
		System.out.println("Enrer an integer");
		int n=in.nextInt();
		
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				System.out.println(i*j+" ");
			}
			System.out.println();
		}
	}
		
}