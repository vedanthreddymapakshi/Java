package testing;
import java.util.*;

public class tt {
	public static void main(String args[])
	{
		int s=0;
		for(int i=10;i<=100;i++)
		{
			boolean b=false;
			
			if(i%4==0&&i%6==0)
			{
				b=true;
			}
			
			if(b==false)
			{
				if(i%4==0||i%6==0)
					s=s+i;
			}
		}
		
		System.out.println(s);
	}
		
}