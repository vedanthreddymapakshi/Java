package testing;

public class tt {
	public static void main(String args[])
	{
		int a=10;
		int b=20;
		int []c={5,10,15,20};
		foo(a,b,c);
		System.out.println(a);
		System.out.println(b);
		
		for(int i=0;i<c.length;i++)
			System.out.println(c[i]);
	}
	public static void foo(int x,int y,int []z)
	{
		x+=5;
		y*=2;
		for(int i=0;i<z.length;i++)
			z[i]-=3;
	}
}